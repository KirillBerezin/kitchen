import math
colors = [int(x) for x in input().split(',')]          #Вводить 2 числа через запятую в 1 строчку
minimal = min(colors)
colors.remove(minimal)
another_c = colors[0]

if another_c >= minimal + 1 and math.sqrt(2 * minimal + 1) == int(math.sqrt(2 * minimal + 1)):
	print(int(math.sqrt(2 * minimal + 1)))
else:
	print(int(math.sqrt(2*minimal)))
